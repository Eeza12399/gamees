   var slug = 'urban-sniper';
   var url = './urban-sniper.html';
   var featuredImage = './media/posts/322/512.jpg';

   var storedData = JSON.parse(localStorage.getItem('postData')) || [];

   var newPostData = {
       title: title,
       slug: slug,
       url: url,
	   featuredImage: featuredImage
   };

   // Check if the post with the same slug already exists and remove it
   storedData = storedData.filter(post => post.slug !== newPostData.slug);

   // Add the new post data to the top of the array
   storedData.unshift(newPostData);

   // Keep only the most recent 30 posts
   if (storedData.length > 30) {
       storedData = storedData.slice(0, 30);
   }

   localStorage.setItem('postData', JSON.stringify(storedData));</script><iframe style="display: none;"></iframe><iframe style="display: none;"></iframe></body></html>
